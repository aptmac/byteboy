{
	"information_for_contributors": [
		"Here is a link to the Byteman Rule Language: https://github.com/bytemanproject/byteman/blob/master/docs/asciidoc/src/main/asciidoc/chapters/Byteman-Rule-Language.adoc"
	],
	"name": "Byteman",
	"scopeName": "source.byteman",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)rule\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)class\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)interface\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)method\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)do\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)if\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)bind\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)at\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)after\\b"
				},
				{
					"name": "keyword.control.btm",
					"match": "\\b^(?i)end\\s*rule\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.btm",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.btm",
					"match": "\\\\."
				}
			]
		}
	}
}